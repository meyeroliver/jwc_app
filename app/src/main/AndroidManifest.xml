        </activity>
        <activity android:name=".ui.activities.LoginActivty" />
        <activity android:name=".ui.activities.ReceiptActivity" />
        <activity android:name=".ui.activities.CartActivity" />
        <activity android:name=".ui.activities.MainActivity"/>

    </application>

</manifest>